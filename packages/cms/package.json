{
  "name": "cms",
  "version": "1.0.0",
  "description": "Payload CMS for PsychCombo content management",
  "main": "dist/server.js",
  "scripts": {
    "dev": "npm run generate:importmap && npm run payload migrate && cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts next dev -p 3000",
    "build": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:importmap && next build",
    "start": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts next start -p 3000",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:importmap": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:importmap",
    "migrate": "tsx src/migrate.ts"
  },
  "type": "module",
  "dependencies": {
    "@payloadcms/db-sqlite": "^3.77.0",
    "@payloadcms/next": "^3.77.0",
    "@payloadcms/richtext-lexical": "^3.77.0",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "better-sqlite3": "^12.6.2",
    "cross-env": "^10.1.0",
    "dotenv": "^16.4.7",
    "express": "^5.2.1",
    "graphql": "^16.13.0",
    "gray-matter": "^4.0.3",
    "next": "15.4.11",
    "payload": "^3.77.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sharp": "^0.34.5",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
